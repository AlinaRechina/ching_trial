NounClass NumNomOblAgr NumInflMatPl NumInflMatDer

NounClass NumNomOblAgr NumInflLocPl               #локатив + согласование (4,14)
NounClass NumNomOblAgr NumInflLocPl NumInflEss
NounClass NumNomOblAgr NumInflLocPl NumInflElat
NounClass NumNomOblAgr NumInflLocPl NumInflTrans
NounClass NumNomOblAgr NumInflLocPl NumInflLat
NounClass NumNomOblAgr NumInflLocPl NumInflLat NumInflTrans
NounClass NumNomOblAgr NumInflLocPl NumInflLat NumInflElat

LEXICON NumAttInfl
<num><att>:

LEXICON NumAbsInfl
<num><abs>:

LEXICON NumInfl
<num><obl><erg>:мма  
<num><obl><gen>:ннан
<num><obl><dat>:нна
<num><obl><ins>:нца

LEXICON NumInflLoc
<num><obl><loc>:нга #местный (allative?)

LEXICON NumInflEss
<ess>:хь #эссив (на месте) (locative?)
LEXICON NumInflLat
<lat>:хьа #латив (в X) (locative?)
LEXICON NumInflElat
<elat>ра #элатив (из Х) (ablative) (в Ингушском может добавляться к  allative, locative, or other oblique forms)
LEXICON NumInflTrans
<trans>:хула #транслатив (через Х)

LEXICON NumInflComp
<num><obl><comp>:ннал #сравнительный
LEXICON NumInflCompDer
<cder>:ла #сравнительный производный

LEXICON NumInflMat
<num><obl><mat>:ннах #вещественный (lative?)
LEXICON NumInflMatDer
<mder>:а #вещественный производный

# МНОЖЕСТВЕННОЕ ЧИСЛО

LEXICON NumAbsInflPl
<num><pl><abs>:ннаш

LEXICON NumInflPl
<num><obl><pl><erg>:ннаша 
<num><obl><pl><gen>:ннийн
<num><obl><pl><dat>:ннашна
<num><obl><pl><ins>:ннашка

LEXICON NumInflLocPl
<num><obl><pl><loc>:нга #местный (allative?)

LEXICON NumInflCompPl
<num><obl><pl><comp>:ннийл #сравнительный

LEXICON NumInflMatPl
<num><obl><pl><mat>:ннийх #вещественный (lative?)

# РОД

LEXICON NounClass
<cl1>:й #ж+ед
<cl2>:б #мн
<cl3>:в #м+ед	
<cl4>:д #мн


